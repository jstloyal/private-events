<div class="jumbotron">
  <h2>User profile page: <%= @user.name if @user %></h2>
  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th>Event name</th>
          <th>Location</th>
          <th>Event Date</th>
          <th>Event Creator</th>
          <th>Attendees</th>
        </tr>
      </thead>
      <tbody>

          <tr><% @created_events.each do |t| %></tr>
            <td><%= t.name %></td>
            <td><%= t.location %></td>
            <td><%= t.date %></td>
            <td><%= t.creator.name %></td>
            <td>
               <% e.attendees.each do |a| %>
                 <%= a.name %>,
               <% end %>
             </td>
          <% end %>


      </tbody>
    </table>
  </div>
</div>
